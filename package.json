{
  "name": "we-vote",
  "description": "",
  "author": "watchout <dev@watchout.tw>",
  "version": "0.0.1",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "karma start",
    "deploy": "babel-node script/gh-pages.es6",
    "dev-mode": "git remote remove origin && git remote add origin git@github.com:watchout-tw/dev.wevote.tw.git",
    "prod-mode": "git remote remove origin && git remote add origin git@github.com:watchout-tw/wevote.tw.git"
  },
  "betterScripts": {
    "start": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "5.8.21",
    "babel-plugin-typecheck": "1.2.0",
    "better-npm-run": "0.0.1",
    "body-parser": "^1.13.2",
    "classnames": "^2.2.0",
    "compression": "^1.5.0",
    "d3": "^3.5.6",
    "express": "^4.13.0",
    "express-session": "^1.11.3",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.8.0",
    "http-proxy": "^1.11.1",
    "lru-memoize": "0.0.2",
    "map-props": "^0.1.1",
    "moment": "^2.10.6",
    "piping": "0.2.0",
    "pretty-error": "^1.1.2",
    "query-string": "^2.4.0",
    "react": "0.13.3",
    "react-document-meta": "^1.1.0",
    "react-inline-css": "1.2.1",
    "react-prefixr": "^0.1.0",
    "react-redux": "0.9.0",
    "react-router": "v1.0.0-beta3",
    "react-swipe": "^2.3.0",
    "redux": "^1.0.1",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.2.0",
    "url-loader": "^0.5.6",
    "webpack-isomorphic-tools": "^0.8.5"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^5.8.22",
    "babel-eslint": "^4.0.10",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.20",
    "chai": "^3.2.0",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "0.1.1",
    "css-loader": "^0.16.0",
    "eslint": "^1.2.0",
    "eslint-config-airbnb": "0.0.7",
    "eslint-plugin-react": "^3.2.3",
    "extract-text-webpack-plugin": "^0.8.1",
    "json-loader": "0.5.2",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "node-sass": "^3.2.0",
    "react-a11y": "0.2.6",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "1.0.2",
    "sass-loader": "^2.0.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "1.10.1"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
